{
  "name": "SimAD",
  "version": "1.0.0",
  "description": "Simulador de Filas com Prioridade - AD 2017.2.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
  "appId": "SimulatorAD",
  "mac": {
      "category": "simulator"
    },
  "win": {
