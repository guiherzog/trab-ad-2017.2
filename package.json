  "keywords": [
    "ad",
    "simulator",
    "ufrj",
    "queue"
  ],
  "author": "Guilherme Herzog, Lucas Carneiro, Eduardo Costa",
  "license": "MIT",
  "devDependencies": {
    "electron": "~1.6.2",
    "electron-builder": "^19.48.3",
    "electron-packager": "^10.1.0"
  },
  "dependencies": {
    "inv-chisquare-cdf": "^1.0.0",
    "seedrandom": "^2.4.3"
  }
}
